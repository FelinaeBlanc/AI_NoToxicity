{

    "description": "Remove all toxic texts",
    "manifest_version": 2,
    "name": "No toxicity",
    "version": "1.0",
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["lib/tf.min.js", "noToxicity.js"],
        "run_at": "document_end"
      }
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": ["storage", "tabs", "activeTab"]
  
  }